{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/cpptorch",
			"args": [
				"experiments/debug.json"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "Python: export_onnx.py",
			"type": "python",
			"request": "launch",
			"program": "scripts/export_onnx.py",
			"args": ["--script_model", "experiments/states/simplenet.pth",
					 "--onnx_file", "experiments/states/simplenet.onnx",
					 "--weight_file", "experiments/states/debug_e5.pkl.210101125900",
					 "--input_shape", "1", "1", "28", "28"],
			"console": "integratedTerminal"
		}
	]
}